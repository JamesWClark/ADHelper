{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ADHelper/bin/Debug/net8.0/ADHelper.dll",
            "args": [
                "-csv", "${workspaceFolder}/TestData/users.csv",
                "-xml", "${workspaceFolder}/TestData/config.xml",
                "-task", "create_users"
            ],
            "cwd": "${workspaceFolder}/ADHelper",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}